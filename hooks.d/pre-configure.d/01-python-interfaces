[ -n "$(find xbmc/interfaces/python -iname Makefile -newer Makefile.in 2>/dev/null)" ] && \
[ -z "$(find xbmc/interfaces/python/generated -maxdepth 1 -iname \*.cpp ! -newer xbmc/interfaces/python/Makefile 2>/dev/null)" ] && return 0

! grep -q '$(DOXY_XML_PATH): $(SWIG) $(JAVA)' codegenerator.mk || { \
    sed -i 's/$(DOXY_XML_PATH): $(SWIG) $(JAVA)/$(DOXY_XML_PATH): $(SWIG)/' codegenerator.mk && ./bootstrap || exit 1; ]

! grep -q 'codegenerated: $(DOXYGEN) $(SWIG) $(JAVA) $(GENERATED)' codegenerator.mk || { \
    sed -i 's/codegenerated: $(DOXYGEN) $(SWIG) $(JAVA) $(GENERATED)/codegenerated: $(DOXYGEN) $(SWIG) $(GENERATED)/' codegenerator.mk && ./bootstrap || exit 1; ]

