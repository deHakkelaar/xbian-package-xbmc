[ -d tools/depends/native/libsquish-native ] || return 0

eval $config_build_env make make -C tools/depends/native/libsquish-native all || \
    ( cd tools/depends/native/; git clean -xdf; \
    cd ../../../..; ./bootstrap; make -C tools/depends/native/JsonSchemaBuilder all )

a=$(find tools/depends/native/libsquish-native/.installed*)
[ -e tools/depends/native/libsquish-native/squish-install ] || \
    ln -s ${a#*.installed-} tools/depends/native/libsquish-native/squish-install
